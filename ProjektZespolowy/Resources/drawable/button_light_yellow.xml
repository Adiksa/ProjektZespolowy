<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="false">
		<layer-list>
			<item android:bottom="2dp" android:left="2dp">
				<shape>
					<gradient android:angle="270" android:endColor="#ece776" android:startColor="#eae7a2"></gradient>
					<stroke android:width="1dp" android:color="#BABABA" />
					<corners android:radius="10dp"/>
					<padding android:bottom="10dp" android:left="10dp" android:top="10dp" android:right="10dp"/>
				</shape>
			</item>
		</layer-list>
	</item>
	<item android:state_pressed="true">
		<layer-list>
			<item android:bottom="2dp" android:left="2dp">
				<shape>
					<gradient android:angle="270" android:endColor="#aba857" android:startColor="#b2b07e"></gradient>
					<stroke android:width="1dp" android:color="#BABABA" />
					<corners android:radius="10dp"/>
					<padding android:bottom="10dp" android:left="10dp" android:top="10dp" android:right="10dp"/>
				</shape>
			</item>
		</layer-list>
	</item>
</selector>